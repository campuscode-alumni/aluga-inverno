<%= form_for [@prop, @proposal] do |f| %>
  <div class="">
    <%= f.label :name, 'Nome' %>
    <%= f.text_field :name %>
  </div>
  <div class="">
    <%= f.label :email, 'email' %>
    <%= f.text_field :email %>
  </div>
  <div class="">
    <%= f.label :cpf, 'cpf' %>
    <%= f.text_field :cpf %>
  </div>
  <div class="">
    <%= f.label :phone, 'telefone' %>
    <%= f.text_field :phone %>
  </div>
  <div class="">
    <%= f.label :start_date, 'data inicial' %>
    <%= f.date_field :start_date %>
  </div>
  <div class="">
    <%= f.label :end_date, 'data final' %>
    <%= f.date_field :end_date %>
  </div>
  <div class="">
    <%= f.label :total_amount, 'total' %>
    <%= f.text_field :total_amount %>
  </div>
  <div class="">
    <%= f.label :rent_purpose, 'proposito' %>
    <%= f.text_field :rent_purpose %>
  </div>
  <div class="">
    <%= f.label :extra_info, 'infs extra' %>
    <%= f.text_field :extra_info %>
  </div>

  <%= f.submit 'Enviar' %>
<% end %>
